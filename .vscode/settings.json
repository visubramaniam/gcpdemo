{
    "ansible.python.interpreterPath": "/opt/homebrew/bin/python3.12",
    "ansible.ansible.path": "/opt/homebrew/bin/ansible",
    "ansible.ansibleNavigator.path": "/opt/homebrew/bin/ansible-navigator",
    "ansible.validation.lint.path": "/opt/homebrew/bin/ansible-lint",
    "ansible.executionEnvironment.enabled": true,
    "ansible.executionEnvironment.containerEngine": "podman",
    "ansible.executionEnvironment.pull.arguments": "--tls-verify=false",
    "ansible.executionEnvironment.pull.policy": "always",
    "ansible.lightspeed.enabled": false,
    "ansible.validation.enabled": true,
    "ansible.validation.lint.enabled": true,
    "ansible.validation.lint.arguments": "--nocolor --parseable --exclude .vscode --exclude .git --exclude .tox --exclude .venv",
}